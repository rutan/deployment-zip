{
  "name": "@rutan/deployment-zip",
  "type": "module",
  "version": "0.3.0",
  "description": "A CLI tool to create a deployment zip file.",
  "author": "ru_shalm <ru_shalm@hazimu.com>",
  "license": "MIT",
  "homepage": "https://github.com/rutan/deployment-zip",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rutan/deployment-zip"
  },
  "bin": {
    "deployment-zip": "bin/deployment-zip"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/index.js",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./plugins": {
      "import": "./lib/plugins/index.js",
      "types": "./lib/plugins/index.d.ts"
    }
  },
  "files": ["bin", "lib", "src"],
  "scripts": {
    "start": "node ./bin/deployment-zip",
    "dev": "jiti ./src/cli.ts",
    "build": "run-s clean build:*",
    "build:tsc": "tsc",
    "lint": "run-s lint:*",
    "lint:biome": "biome check ./",
    "lint:tsc": "tsc --noEmit",
    "format": "biome check ./ --apply",
    "clean": "rimraf ./lib"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.529.1",
    "@aws-sdk/credential-provider-node": "^3.529.1",
    "@aws-sdk/lib-storage": "^3.529.1",
    "archiver": "^7.0.0",
    "citty": "^0.1.6",
    "consola": "^3.2.3",
    "ignore": "^5.3.1",
    "jiti": "^1.21.0",
    "mime": "^4.0.1",
    "node-html-parser": "^6.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.0",
    "@biomejs/cli-win32-x64": "^1.6.0",
    "@types/archiver": "^6.0.2",
    "@types/node": "^20.11.25",
    "npm-run-all2": "^6.1.2",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.2"
  },
  "packageManager": "pnpm@8.15.1"
}
